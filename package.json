    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.77.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.11.0",
    "jsdom": "^24.0.0",
    "rimraf": "^5.0.7",
    "sass-loader": "^14.2.0",
    "style-loader": "^3.3.4",
