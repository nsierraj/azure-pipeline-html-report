  },
  "scripts": {
    "version:sync": "node scripts/sync-version.js",
    "version": "node scripts/sync-version.js && git add azure-devops-extension.json PublishHtmlReport/task.json PublishHtmlReport/package.json",
    "clean": "rimraf ./dist",
    "build:cidev": "npm run clean && webpack --mode development",
    "build": "npm run clean && webpack --mode development",
    "postbuild": "npm run package-extension",
    "postinstall": "cd PublishHtmlReport && npm install",
    "test": "echo No test available",
    "prune": "npm prune --production --ignore-scripts",
    "package-extension": "tfx extension create --manifest-globs azure-devops-extension.json --overrides-file dev_manifest.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
